import #strings as str

entry {
    "abc" !! dup putsln ;; string explode  -- "abc" -> ["a", "b", "c"]
    .. putsln           ;; implode it back -- ["a", "b", "c"] -> ["abc"]
    [1, 2, 3] [4, 5, 6] : putsln ;; list concatenation

    try {
        [1, 2, 3] ["4", 5, "6"] : putsln
    } else err {
        "This error occured: " puts err putsln
    }

    ;; `concat` in #strings library
    "abc" "def" !! swap !! : .. putsln
    "abc" "def" str::concat putsln
}